    "scripts": {
        "lint": "eslint .",
        "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
        "ts": "ts-node --project tsconfig-node.json ",
        "prepare": "husky",
        "pureJs-cli": "node ./.tools/esbuild/pureJs/cli.mjs",
